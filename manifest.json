{
    "manifest_version":2,
    "name":"Anonymous Quora Browsing",
    "version":"4.0",
    "author":" ismnoiet <ismnoiet@gmail.com>",
    "description":"Browse quora threads anonymously without having to log in",
    "permissions":[
        "webRequest",
        "webRequestBlocking",
        "https://www.quora.com/**"
    ],
    "page_action":{
        "default_title":"Quora anonymous browsing"
    },
    "background":{
        "scripts":["background.js"]
    },
    "icons": {
        "16": "icons/icon16.png",
        "48":"icons/icon48.png",
        "128": "icons/icon128.png"
    }
}